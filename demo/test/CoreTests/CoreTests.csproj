<Project ToolsVersion="15.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFrameworkVersion>v4.7.2</TargetFrameworkVersion>
    <GenerateTargetFrameworkAttribute>false</GenerateTargetFrameworkAttribute>
    <AssemblyName>CoreTests</AssemblyName>
    <RootNamespace>CoreTests</RootNamespace>
    <!-- 禁止自动引用系统 mscorlib -->
    <NoStandardLib>true</NoStandardLib>
    <NoConfig>true</NoConfig>
    <OutputPath>bin\Debug\</OutputPath>
    <ProjectGuid>{A098E4A9-8B0E-4612-A75E-0AE2061E70FF}</ProjectGuid>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)' == 'Debug'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="mscorlib">
      <HintPath>$(ProjectDir)\..\..\win64\dotnetframework\mscorlib.dll</HintPath>
    </Reference>
    <Reference Include="System.Core">
      <HintPath>$(ProjectDir)\..\..\win64\dotnetframework\System.Core.dll</HintPath>
    </Reference>
    <Reference Include="System">
      <HintPath>$(ProjectDir)\..\..\win64\dotnetframework\System.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Compile Include="App.cs" />
    <Compile Include="Assert.cs" />
    <Compile Include="Properties\AssemblyInfo.cs" />
  </ItemGroup>
  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
</Project>